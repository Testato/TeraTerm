        MENUITEM "Version ",                    ID_VERSION
        MENUITEM SEPARATOR
        MENUITEM "Execute",                     ID_EXEC
        MENUITEM SEPARATOR
        MENUITEM "Quit",                        ID_TMENU_CLOSE
    END
END


