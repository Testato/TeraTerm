BEGIN
    POPUP "MENU"
    BEGIN
        MENUITEM "List Configuration",          ID_TMENU_ADD
        POPUP "Setting"
        BEGIN
            MENUITEM "showing large icon",          ID_ICON
            MENUITEM "specifying font",             ID_FONT
            MENUITEM "showing list by left-click",  ID_LEFTPOPUP
